{
  "name": "vtg_starter_app",
  "version": "1.0.0",
  "description": "A starter Vue + Tailwind + Google Apps Script app",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./client/webpack.dev.js ",
    "local": "webpack-dev-server --config ./client/webpack.dev.js --open \"Firefox\"",
    "prod": "webpack --config ./client/webpack.prod.js ",
    "gas": "webpack --config ./server/webpack.gas.js ",
    "deploy": "npm run prod &&  npm run gas && cd deploy/gas && rm app.bundle.js && clasp push && clasp open --webapp"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^2.1.1",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "5.0.0",
    "popper.js": "^1.14.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "autoprefixer": "^9",
    "core-js": "^3.8.1",
    "postcss": "^7",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.1.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "uglifyjs-webpack-plugin": "^2.2.0"
  }
}
